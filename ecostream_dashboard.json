{
    "title": "EcoStream Global Sustainability Monitor",
    "description": "Enterprise Observability for SDG 12 Navigator.",
    "widgets": [
        {
            "definition": {
                "title": "Gemini 2.5 Model Latency (p95)",
                "type": "timeseries",
                "requests": [
                    {
                        "q": "avg:gcp.aiplatform.publisher.online_serving.model_invocation_latencies.p95{project_id:tag-file-manager}",
                        "display_type": "line"
                    }
                ]
            },
            "layout": {
                "x": 0,
                "y": 0,
                "width": 6,
                "height": 2
            }
        },
        {
            "definition": {
                "title": "Total Eco-Actions Persisted (BigQuery)",
                "type": "query_value",
                "requests": [
                    {
                        "q": "sum:gcp.bigquery.storage.insertall_inserted_rows{project_id:tag-file-manager}",
                        "aggregator": "sum"
                    }
                ],
                "precision": 0
            },
            "layout": {
                "x": 6,
                "y": 0,
                "width": 3,
                "height": 2
            }
        },
        {
            "definition": {
                "title": "Real-time Map Visualizations",
                "type": "query_value",
                "requests": [
                    {
                        "q": "sum:gcp.maps.service.v2.request_count{project_id:tag-file-manager}",
                        "aggregator": "sum"
                    }
                ],
                "precision": 0
            },
            "layout": {
                "x": 9,
                "y": 0,
                "width": 3,
                "height": 2
            }
        }
    ]
}